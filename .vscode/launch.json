{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "mocha[debug]",
      "program": "${workspaceFolder}/node_modules/mocha/bin/_mocha",
      "args": [
        "--no-timeouts",
        "--colors",
        "--require",
        "espower-typescript/guess",
        "${relativeFile}"
      ],
      "console": "integratedTerminal"
    }
  ]
}
